{
  "name": "express",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "clean": "rimraf dist",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "dev": "tsc-watch --onSuccess \"node ./dist/server.js\"",
    "build": "tsc --p tsconfig.build.json",
    "build:start": "npm run build & npm run start",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "test_sample": "mocha -r ts-node/register test/sample.spec.ts",
    "test_sample_error": "mocha -r ts-node/register test/sample-error.spec.ts",
    "test_parallel": "mocha --parallel -r ts-node/register test/**/*.spec.ts",
    "test_one": "mocha -r ts-node/register test/one-to-one.spec.ts",
    "test_one_many": "mocha -r ts-node/register test/one-to-many.spec.ts",
    "test_many_many": "mocha -r ts-node/register test/many-to-many.spec.ts",
    "test:coverage": "nyc npm run test",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:run": "typeorm migration:run",
    "build:migration:run": "npm run build & typeorm migration:run"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/!(*.test.*).[tj]s?(x)"
    ],
    "exclude": [
      "src/_tests_/**/*.*"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage"
  },
  "dependencies": {
    "@types/compression": "^1.7.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.1",
    "@types/faker": "^5.5.9",
    "@types/http-errors": "^1.8.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.3.1",
    "@types/supertest": "^2.0.11",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "debug": "~4.3.2",
    "dotenv": "^10.0.0",
    "express": "~4.17.1",
    "faker": "^5.5.3",
    "http-errors": "~1.8.0",
    "jade": "~1.11.0",
    "morgan": "~1.10.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.3",
    "tsc-watch": "^4.4.0",
    "typeorm": "^0.2.34",
    "typescript": "4.3.5"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^8.2.3",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "chai": "4.3.4",
    "eslint": "^7.30.0",
    "mocha": "9.0.2",
    "nyc": "^15.1.0",
    "prettier": "2.3.2",
    "ts-node": "^10.1.0"
  }
}
