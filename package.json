{
  "name": "express",
  "version": "0.0.4",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "clean": "rimraf dist",
    "lint": "eslint . --ext=.ts",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "dev": "tsc-watch --onSuccess \"node ./dist/src/server.js\"",
    "build": "tsc --p tsconfig.build.json",
    "build:start": "npm run build & npm run start",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "test:parallel": "mocha --parallel -r ts-node/register test/**/*.spec.ts",
    "test_sample": "mocha -r ts-node/register test/sample.spec.ts",
    "test_one_to_one": "mocha -r ts-node/register test/one-to-one.spec.ts",
    "test_one_to_many": "mocha -r ts-node/register test/one-to-many.spec.ts",
    "test_many_to_many": "mocha -r ts-node/register test/many-to-many.spec.ts",
    "test:coverage": "nyc npm run test",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run:arg": "typeorm migration:run -d dist/data_source/arg.source.js",
    "migration:run:mx": "typeorm migration:run -d dist/data_source/mx.source.js",
    "migration:show:arg": "typeorm migration:show -d dist/data_source/arg.source.js",
    "migration:show:mx": "typeorm migration:show -d dist/data_source/mx.source.js",
    "migration:run": "typeorm migration:run -d dist/data-source.js",
    "build:migration:run": "npm run build & migration:run"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.1",
    "@types/http-errors": "^1.8.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.3.1",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "debug": "~4.3.2",
    "dotenv": "^10.0.0",
    "express": "~4.17.1",
    "express-validator": "^6.14.0",
    "helmet": "^4.6.0",
    "http-errors": "~1.8.0",
    "jade": "^0.31.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.10.0",
    "mysql": "^2.18.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "tsc-watch": "^4.4.0",
    "typeorm": "0.3.5",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.2",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.0",
    "@types/chance": "^1.1.3",
    "@types/mocha": "^9.1.0",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "chai": "4.3.4",
    "chance": "^1.1.8",
    "eslint": "^8.9.0",
    "mocha": "9.2.0",
    "nyc": "^15.1.0",
    "prettier": "2.5.1",
    "supertest": "^6.2.2",
    "ts-node": "^10.7.0"
  }
}
