{
  "name": "express",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "clean": "rimraf dist",
    "lint": "eslint . --ext=.ts",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "dev": "tsc-watch --onSuccess \"node ./dist/src/server.js\"",
    "build": "tsc --p tsconfig.build.json",
    "build:start": "npm run build & npm run start",
    "test": "mocha --require test/fixtures.cjs -r ts-node/register test/**/*.spec.ts",
    "test:coverage": "nyc npm run test",
    "test:parallel": "mocha --parallel --require test/fixtures.cjs -r ts-node/register test/**/*.spec.ts",
    "test_sample": "mocha -r ts-node/register test/sample.spec.ts",
    "test_only_file": "mocha --require test/fixtures.cjs -r ts-node/register test/sample.spec.ts"
  },
  "dependencies": {
    "@types/compression": "^1.7.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.1",
    "@types/http-errors": "^1.8.1",
    "@types/morgan": "^1.9.3",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "debug": "~4.3.2",
    "dotenv": "^10.0.0",
    "express": "~4.17.1",
    "express-validator": "^6.14.0",
    "helmet": "^4.6.0",
    "http-errors": "~1.8.0",
    "jade": "^0.31.2",
    "morgan": "~1.10.0",
    "rate-limiter-flexible": "^2.3.6",
    "rimraf": "^3.0.2",
    "tsc-watch": "^4.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.2",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.0",
    "@types/chance": "^1.1.3",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.3.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "chai": "4.3.4",
    "chance": "^1.1.8",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "mocha": "9.2.0",
    "nyc": "^15.1.0",
    "prettier": "2.5.1",
    "supertest": "^6.2.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.6.0"
  }
}
